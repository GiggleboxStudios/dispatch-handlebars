{
  "name": "giggleboxstudios/dispatch-handlebars"
, "type": "library"
, "description": "handlebars support for dispatch micro framework"
, "keywords": ["dispatch", "handlebars", "template"]

, "homepage": "http://giggleboxstudios.github.io/dispatch-handlebars/"
, "license": "MIT"

, "authors": [{
      "name": "Brandtley McMinn"
    , "role": "Developer"
    , "homepage": "http://giggleboxstudios.github.io/"
    }]

, "require": {
