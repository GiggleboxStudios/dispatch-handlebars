    , "dispatch/dispatch":      ">= 2.6.2"
    , "xamin/handlebars.php":   "dev-master"
    }

, "autoload": {
      "files": [
        "src/dispatch-handlebars.php"
      ]
    }

, "extra": {
      "branch-alias": {
        "dev-master": "0.x-dev"
      }
    }

}
